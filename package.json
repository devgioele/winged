{
  "name": "@devgioele/winged",
  "version": "7.0.6",
  "description": "A blazingly fast GeoJSONL processor to group features",
  "license": "LGPL-3.0-or-later",
  "keywords": [
    "gis",
    "geojson",
    "geojsonl"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/devgioele/winged.git"
  },
  "author": "devgioele",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint src/**/*.ts --cache --fix",
    "test": "rimraf __tests__/out && jest",
    "cleanup": "rimraf dist",
    "build": "yarn cleanup && yarn build:cjs & yarn build:esm & yarn build:types",
    "build:cjs": "node ./esbuild.cjs.js",
    "build:esm": "node ./esbuild.esm.js",
    "build:types": "tsc -p tsconfig.types.json",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "17.0.36",
    "esbuild": "0.14.42",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.4.5",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.3",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.8"
  }
}
